#include <iostream>

using namespace std;

int main()
{

    int n , s ,crim=0,ofi=0 ;
    cin >> n;
    while(n--){
        cin>>s;
        if(s<0){

            if(ofi>0){
                ofi--;
            }

            else
                crim++;
        }
        else
            ofi=ofi+s;
    }
    cout<<crim;

    return 0;
}
